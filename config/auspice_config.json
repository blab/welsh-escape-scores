{
  "title": "Real-time tracking of influenza A/H3N2 evolution",
  "maintainers": [
    {"name": "John Huddleston", "url": "https://bedford.io/team/john-huddleston/"},
    {"name": "Jennifer Chang", "url": "https://bedford.io/team/jennifer-chang/"},
    {"name": "Jover Lee", "url": "https://bedford.io/team/jover-lee/"},
    {"name": "Richard Neher", "url": "https://neherlab.org/richard-neher.html"},
    {"name": "Trevor Bedford", "url": "https://bedford.io/team/trevor-bedford/"}
  ],
  "data_provenance": [
    {
      "name": "GISAID"
    }
  ],
  "build_url": "https://github.com/blab/welsh-escape-scores",
  "colorings": [
    {
      "key": "gt",
      "title": "Genotype",
      "type": "categorical"
    },
    {
      "key": "num_date",
      "title": "Date",
      "type": "continuous"
    },
    {
      "key": "historicalclade",
      "title": "Historical clade",
      "type": "categorical"
    },
    {
      "key": "subclade",
      "title": "Subclade",
      "type": "categorical"
    },
    {
      "key": "weighted_distance_to_observed_future",
      "title": "Distance to observed future population",
      "type": "continuous"
    },
    {
      "key": "welsh_ep",
      "title": "Welsh et al. epitope mutations",
      "type": "continuous"
    },
    {
      "key": "welsh_escape",
      "title": "Welsh et al. escape score (avg)",
      "type": "continuous"
    },
    {
      "key": "welsh_escape_upper_80th_quantile",
      "title": "Welsh et al. escape score (80th pct)",
      "type": "continuous"
    },
    {
      "key": "welsh_escape_youth",
      "title": "Welsh et al. escape score (ages 2-20)",
      "type": "continuous"
    },
    {
      "key": "ha1",
      "title": "HA1 mutations",
      "type": "continuous"
    },
    {
      "key": "welsh_escape_per_ha1",
      "title": "Welsh et al. escape score (avg) per HA1 subs",
      "type": "continuous"
    },
    {
      "key": "welsh_escape_upper_80th_quantile_per_ha1",
      "title": "Welsh et al. escape score (80th pct) per HA1 subs",
      "type": "continuous"
    },
    {
      "key": "welsh_escape_youth_per_ha1",
      "title": "Welsh et al. escape score (ages 2-20) per HA1 subs",
      "type": "continuous"
    },
    {
      "key": "lbi",
      "title": "Local branching index (LBI)",
      "type": "continuous"
    },
    {
      "key": "region",
      "title": "Region",
      "type": "categorical"
    },
    {
      "key": "country",
      "title": "Country",
      "type": "categorical"
    },
    {
      "key": "submitting_lab",
      "title": "Submitting lab",
      "type": "categorical"
    },
    {
      "key": "originating_lab",
      "title": "Originating lab",
      "type": "categorical"
    }
  ],
  "geo_resolutions": [
    "country",
    "region"
  ],
  "display_defaults": {
    "map_triplicate": true,
    "color_by": "historicalclade",
    "branch_label": "historicalclade"
  },
  "filters": [
    "historicalclade",
    "subclade",
    "region",
    "country",
    "submitting_lab"
  ],
  "panels": [
    "tree",
    "measurements",
    "entropy",
    "frequencies"
  ]
}
